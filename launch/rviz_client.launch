<launch>
    <!-- 
    launch_prefix: {xterm -e gdb -ex run -args} 
    rtabmap_args:  uinfo, udebug
    -->
    <arg name="output"                  default="screen"/> 
    <arg name="launch_prefix"           default=""/>      
   
   
    <!-- Visualisation RVIZ -->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find rtabmap_ros)/launch/config/rs_d400_t265.rviz" output="$(arg output)" launch-prefix="$(arg launch_prefix)"/>
    

</launch>